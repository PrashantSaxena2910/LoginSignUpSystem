<body>
<h1> Sign up form </h1>

<form action="/register" method="POST">
	
		<div class="form-group">
		  <label for="exampleInputEmail1">User Name</label>
		  <input type="text" name="username" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter username">
		  <small id="emailHelp" class="form-text text-muted">We'll never share your name with anyone else.</small>
		</div>
		<div class="form-group">
		  <label for="exampleInputPassword1">Password</label>
		  <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
		</div>
		
		<button type="submit" class="btn btn-primary">SignUp</button>
	  
</form>



<div class="card-body">
	<div class="btn-group" role="group" aria-label="Basic example">
	<a href="/register" class="btn btn-outline-primary" aria-current="page">Sign up</a>
	<a href="/login" class="btn btn-outline-primary" aria-current="page">Login</a>
	
	<a href="/home"  class="btn btn-outline-primary" aria-current="page">Home</a>
	</div>
</div>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm" crossorigin="anonymous"></script>

